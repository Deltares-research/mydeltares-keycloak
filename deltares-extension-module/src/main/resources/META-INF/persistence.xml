<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">

<persistence-unit name="deltares-extentsions">

    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
    <class>nl.deltares.keycloak.storage.jpa.Mailing</class>
    <class>nl.deltares.keycloak.storage.jpa.UserMailing</class>
    <class>nl.deltares.keycloak.storage.jpa.Avatar</class>
    <properties>
        <property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect" />
    </properties>
</persistence-unit>
</persistence>